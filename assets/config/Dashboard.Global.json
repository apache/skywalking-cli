{
  "metrics": [
    {
      "condition": {
        "name": "service_cpm",
        "normal": true,
        "scope": "Service",
        "topN": 10,
        "order": "DES"
      },
      "title": " Service Load (calls/min) "
    },
    {
      "condition": {
        "name": "service_resp_time",
        "normal": true,
        "scope": "Service",
        "topN": 10,
        "order": "DES"
      },
      "title": "    Slow Services (ms)    "
    },
    {
      "condition": {
        "name": "service_apdex",
        "normal": true,
        "scope": "Service",
        "topN": 10,
        "order": "ASC"
      },
      "title": "Un-Health Services (Apdex)",
      "aggregation": "/",
      "aggregationNum": "10000"
    },
    {
      "condition": {
        "name": "endpoint_avg",
        "normal": true,
        "scope": "Endpoint",
        "topN": 10,
        "order": "DES"
      },
      "title": "    Slow Endpoints (ms)   "
    }
  ],
  "responseLatency": {
    "condition": {
      "name": "all_percentile",
      "entity": {
        "scope": "All",
        "normal": true
      }
    },
    "labels": "0, 1, 2, 3, 4",
    "title": "Global Response Latency",
    "unit": "percentile in ms"
  },
  "heatMap": {
    "condition": {
      "name": "all_heatmap",
      "entity": {
        "scope": "All",
        "normal": true
      }
    },
    "title": "Global Heatmap",
    "unit": "ms"
  }
}
